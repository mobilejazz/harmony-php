{
  "authors": [
    {
      "email": "info@mobilejazz.com",
      "name": "Mobile Jazz"
    }
  ],
  "autoload": {
    "psr-4": {
      "Harmony\\Core\\": "core/",
      "Sample\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "description": "Harmony PHP Sample",
  "license": "MIT",
  "minimum-stability": "dev",
  "name": "harmony/sample",
  "prefer-stable": true,
  "require": {
    "ext-pdo": "*",
    "latitude/latitude": "4.2.*",
    "nesbot/carbon": "2.64.*",
    "php": "8.1.*",
    "symfony/framework-bundle": "5.4.*",
    "symfony/property-access": "5.4.*",
    "symfony/serializer": "5.4.*",
    "symfony/validator": "5.4.*"
  },
  "require-dev": {
    "pestphp/pest": "1.22.*",
    "phpstan/phpstan": "1.9.*",
    "psalm/plugin-phpunit": "0.18.*",
    "phpunit/phpunit": "9.5.*",
    "roave/security-advisories": "dev-latest",
    "vimeo/psalm": "5.1.*"
  },
  "type": "project"
}
