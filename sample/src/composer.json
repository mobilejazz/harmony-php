{
    "name": "harmony/sample",
    "description": "Harmony PHP Sample",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Mobile Jazz",
            "email": "info@mobilejazz.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "8.0.*",
        "nesbot/carbon": "2.42.*"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "vimeo/psalm": "4.3.*",
        "phpstan/phpstan": "0.12.*",
        "phpmd/phpmd": "2.9.*",
        "phpunit/phpunit": "9.3.*",
        "pestphp/pest": "0.3.*",
        "squizlabs/php_codesniffer": "3.5.*"
    },
    "autoload": {
        "psr-4": {
            "Sample\\": "app/",
            "harmony\\core\\": "core/",
            "harmony\\eloquent\\": "eloquent/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    }
}
