{
  "name": "harmony/sample",
  "description": "Harmony PHP Sample",
  "type": "project",
  "license": "MIT",
  "authors": [
    {
      "name": "Mobile Jazz",
      "email": "info@mobilejazz.com"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "8.0.*",
    "nesbot/carbon": "2.42.*"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "vimeo/psalm": "4.7.*",
    "phpstan/phpstan": "0.12.*",
    "phpmd/phpmd": "@stable",
    "phpunit/phpunit": "9.5.*",
    "pestphp/pest": "1.0.*",
    "squizlabs/php_codesniffer": "3.6.*"
  },
  "autoload": {
    "psr-4": {
      "Sample\\": "app/",
      "harmony\\core\\": "core/",
      "harmony\\eloquent\\": "eloquent/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  }
}
