{
  "authors": [
    {
      "email": "info@mobilejazz.com",
      "name": "Mobile Jazz"
    }
  ],
  "autoload": {
    "psr-4": {
      "Harmony\\Core\\": "core/",
      "Sample\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "description": "Harmony PHP Sample",
  "license": "MIT",
  "minimum-stability": "dev",
  "name": "harmony/sample",
  "prefer-stable": true,
  "require": {
    "latitude/latitude": "4.2.*",
    "nesbot/carbon": "2.60.*",
    "php": "8.1.*",
    "symfony/framework-bundle": "6.1.*",
    "symfony/property-access": "6.1.*",
    "symfony/serializer": "6.1.*",
    "symfony/validator": "6.1.*"
  },
  "require-dev": {
    "pestphp/pest": "1.21.*",
    "phpmd/phpmd": "2.12.*",
    "phpstan/phpstan": "1.8.*",
    "phpunit/phpunit": "9.5.*",
    "roave/security-advisories": "dev-latest",
    "squizlabs/php_codesniffer": "3.7.*",
    "vimeo/psalm": "4.26.*"
  },
  "type": "project"
}
